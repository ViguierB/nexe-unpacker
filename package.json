{
  "name": "nexe_unpacker",
  "version": "0.0.2",
  "description": "A simple nexe unpacker",
  "main": "index.js",
  "bin": {
    "nexe_unpacker": "index.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node ./index.js",
    "configure": "npm i",
    "prepackage": "npm run build",
    "package": "rm -fr package && mkdir package && cp -r package.json LICENSE index.js dist package/",
    "predopublish": "npm run package",
    "dopublish": "cd package && npm publish",
    "test": "mocha -r ts-node/register **/*.spec.ts"
  },
  "author": "Benjamin Viguier <ben@holidev.net>",
  "repository": {
    "type": "git",
    "url": "git://gitlab.holidev.net/ben/nexe-unpacker"
  },
  "homepage": "https://gitlab.holidev.net/ben/nexe-unpacker#readme",
  "license": "ISC",
  "devDependencies": {
    "@types/archiver": "^5.3.1",
    "@types/mocha": "^9.0.0",
    "@types/node": "^17.0.8",
    "@types/yargs": "^17.0.8",
    "mocha": "^9.1.3",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "archiver": "^5.3.0",
    "reflect-metadata": "^0.1.13",
    "tsyringe": "^4.6.0",
    "yargs": "^17.3.1"
  }
}
